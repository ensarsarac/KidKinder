@model KidKinderProject.Entities.Gallery
@{
    ViewBag.Title = "CreateImage";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="card">
    <div class="card-header">
        <h5>Yeni Resim Ekleme Sayfası</h5>
    </div>
    <div class="card-block">
        <form class="form-material" method="post">
            <div class="form-group form-default">
                <input type="text" name="Path" placeholder="Resim Yolu " class="form-control">
            </div>
           

            <button type="submit" class="btn btn-outline-primary">Resmi Kaydet</button>
        </form>
    </div>
</div>
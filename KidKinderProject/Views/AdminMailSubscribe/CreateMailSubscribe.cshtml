@model KidKinderProject.Entities.MailSubscribe
@{
    ViewBag.Title = "CreateMailSubscribe";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<div class="card">
    <div class="card-header">
        <h5>Yeni Mail Bülteni Girişi</h5>
    </div>
    <div class="card-block">
        <form class="form-material" method="post">
            <span class="text-danger">
            </span>
            <div class="form-group form-default">
                <input type="text" name="NameSurname" placeholder="Adınız Soyadınız" class="form-control">
                <span class="form-bar"></span>
            </div>
            <div class="form-group form-default">
                <input type="text" name="Email" placeholder="Mail Adresiniz" class="form-control">
                <span class="form-bar"></span>
            </div>           
            <button type="submit" class="btn btn-outline-primary">Kaydet</button>
        </form>
    </div>
</div>
@model KidKinderProject.Entities.Branch
@{
    ViewBag.Title = "CreateBranch";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="card">
    <div class="card-header">
        <h5>Yeni Branş Ekleme Sayfası</h5>
    </div>
    <div class="card-block">
        <form class="form-material" method="post">
            <div class="form-group form-default">
                <input type="text" name="BranchName" placeholder="Branş Adı" class="form-control">
            </div>            

            <button type="submit" class="btn btn-outline-primary">Branşı Kaydet</button>
        </form>
    </div>
</div>
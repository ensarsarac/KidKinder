
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

@Html.Partial("~/Views/AdminLayout/_AdminHeadPartial.cshtml")

<body>
    <!-- Pre-loader start -->
    @Html.Partial("~/Views/AdminLayout/_AdminLoaderPartial.cshtml")
    <!-- Pre-loader end -->
    <div id="pcoded" class="pcoded">
        <div class="pcoded-overlay-box"></div>
        <div class="pcoded-container navbar-wrapper">
            @Html.Partial("~/Views/AdminLayout/_AdminNavbarPartial.cshtml")

            <div class="pcoded-main-container">
                <div class="pcoded-wrapper">
                    @Html.Action("_AdminSideBarPartial","AdminLayout")
                    <div class="pcoded-content">
                        <!-- Page-header start -->
                        @Html.Partial("~/Views/AdminLayout/_AdminSamplePagePartial.cshtml")
                        <!-- Page-header end -->
                        <div class="pcoded-inner-content">
                            <div class="main-body">
                                <div class="page-wrapper">
                                    <div class="page-body">
                                        <div class="row">
                                            <div class="col-sm-12">
                                                @RenderBody()
                                            </div>

                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div id="styleSelector"></div>
                </div>
            </div>
        </div>
    </div>
    @Html.Partial("~/Views/AdminLayout/_AdminScriptPartial.cshtml")
</body>

</html>

